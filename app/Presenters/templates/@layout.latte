{*
<script>
	var pageInitializer = {
		initialized: false,
		before: {},
		beforeCounter: {},
		after: {},
		afterCounter: {},
		register: function (callback, name, section) {
			section = (typeof section == "undefined") ? "after" : section;
			this[section][name] = callback;
			if (this.initialized) {
				this.perform(callback, name, section);
			}
		},
		perform: function (callback, name, section) {
			callback(name);
			if (this[section + "Counter"][name] == undefined) {
				this[section + "Counter"][name] = 1;
			} else {
				this[section + "Counter"][name]++;
			}
		}
	};
</script>
*}
