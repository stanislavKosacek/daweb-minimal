{snippet scripts}
{block scripts}
	<script>
		document.addEventListener("DOMContentLoaded", function () {
			raiseModal();
		})

		naja.snippetHandler.addEventListener("afterUpdate", function () {
			raiseModal();
			pageLazyLoad.update();
		});

		function raiseModal() {
			var el = document.querySelector(".inmodal");
			var close = document.querySelector('.inmodal .close-modal');

			if (el) {
				var modal = new Modal(el);
				modal.show();
				// showModal.modal();
				el.addEventListener('hide.bs.modal', function (e) {
					if (!e.target.classList.contains("datepicker")) {
						close.click();
					}
				});
			}
		}

	</script>
{/block}
{/snippet}

